{
    "name": "starweb/starweb-sdk",
    "description": "Starweb SDK for PHP",
    "keywords": ["ecommerce", "shop", "starweb", "api"],
    "homepage": "https://www.starweb.com/",
    "authors": [
        {
            "name": "Starweb AB",
            "email": "starwebapi@starweb.se"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.4",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.7",
        "php-http/cache-plugin": "^1.5",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/cache": "^4.1",
        "symfony/console": "^4.1",
        "symfony/serializer": "^4.1",
        "symfony/property-access": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "php-http/guzzle6-adapter": "^1.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {"Starweb\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Starweb\\Tests\\": "tests/unit"}
    }
}
